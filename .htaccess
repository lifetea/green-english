<IfModule mod_rewrite.c>
  Options +FollowSymlinks
  RewriteEngine On

  RewriteCond %{REQUEST_FILENAME} !-d
  RewriteCond %{REQUEST_FILENAME} !-f

  RewriteRule ^(.*)$ index.php/Home/Index/$1 [QSA,PT,L]
  RewriteRule ^about_us.php$ about_us
  RewriteRule ^Download.php$ download
  RewriteRule ^Contact_us.php$ contact_us
  RewriteRule ^Support.php$ support
  RewriteRule ^news_Company.php$ company
  RewriteRule ^news_Industry.php$ industry

</IfModule>